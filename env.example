# Example environment variables for Lakehead University Chatbot
# Copy this file to backend/.env and fill in your actual values

# Dialogflow CX Configuration
DIALOGFLOW_PROJECT_ID=your-project-id
DIALOGFLOW_LOCATION=us-central1
DIALOGFLOW_AGENT_ID=your-agent-id

# Credentials file path (relative to backend/)
DIALOGFLOW_CREDENTIALS=dialogflow_key.json

# Session settings
SESSION_ID=chat-bot-session
DIALOGFLOW_LANGUAGE_CODE=en-US
